    "test": "jest --passWithNoTests",
    "build": "npm run clean && npm run tailwind && rollup -c --bundleConfigAsCjs",
    "tailwind": "npx tailwindcss -i ./tailwind-input.css -o ./src/styles/globals.css",
    "watch:tailwind": "npx tailwindcss -i ./tailwind-input.css -o ./src/styles/globals.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook"
